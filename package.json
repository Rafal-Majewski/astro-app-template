{
	"name": "astro-app-template",
	"version": "0.0.0",
	"description": "Astro.js app template",
	"private": true,
	"scripts": {
		"dev": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"test": "vitest --run --passWithNoTests --coverage",
		"compile": "tsc",
		"vitest:check": "vitest --run --passWithNoTests --coverage",
		"vitest:check:watch": "vitest --watch --passWithNoTests --coverage",
		"prettier:fix": "prettier --write .",
		"prettier:check": "prettier --check .",
		"eslint:fix": "eslint --fix .",
		"eslint:check": "eslint --max-warnings 0 .",
		"typescript:check": "tsc --noEmit",
		"npm-audit:check": "npm audit --audit-level=low",
		"formatting:fix": "prettier --write .",
		"formatting:check": "prettier --check .",
		"compile:check": "tsc --noEmit",
		"release": "release-it",
		"hadolint:check": "hadolint Dockerfile"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Rafal-Majewski/astro-app-template.git"
	},
	"author": "Rafa≈Ç Majewski",
	"license": "CC0-1.0",
	"bugs": {
		"url": "https://github.com/Rafal-Majewski/astro-app-template/issues"
	},
	"homepage": "https://github.com/Rafal-Majewski/astro-app-template#readme",
	"devDependencies": {
		"@types/node": "^18.16.0",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"@vitest/coverage-c8": "^0.29.3",
		"astro": "^2.3.0",
		"eslint": "^8.37.0",
		"prettier": "^2.8.7",
		"release-it": "^15.9.0",
		"typescript": "^5.0.3",
		"vitest": "^0.30.1"
	},
	"type": "module"
}
